<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IApplicationService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:8733/Kask.Services/Application/"
          binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IApplicationService"
          contract="ApplicationServiceRef.IApplicationService" name="BasicHttpBinding_IApplicationService" />
    </client>
  </system.serviceModel>

  <system.web>
    <httpRuntime maxRequestLength="4000"
      enable = "true"
      requestLengthDiskThreshold="512" shutdownTimeout="90"
      executionTimeout="110" />
  </system.web>

</configuration>